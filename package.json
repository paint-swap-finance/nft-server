{
  "name": "nft-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@sentry/serverless": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@walletconnect/web3-provider": "^1.7.0",
    "aws-sdk": "^2.981.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "electron": "^16.0.4",
    "encoding": "^0.1.13",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "moralis": "^0.0.78",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "web3": "^1.6.1"
  },
  "scripts": {
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy:dev": "export NODE_ENV=dev && npm run deploy:env",
    "deploy:prod": "export NODE_ENV=prod && npm run deploy:env",
    "deploy": "npm run deploy:dev",
    "serve": "serverless offline start",
    "dev": "ts-node src/app.ts",
    "build": "tsc",
    "dist": "sh ./scripts/dist.sh",
    "start": "node dist/src/app.js",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "test": "npx ts-node test/test.ts",
    "invoke": "serverless invoke local --function triggerAdapter --data '{\"adapterIndex\": 0}' "
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.87",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "serverless": "^2.68.0",
    "serverless-offline": "^8.3.1",
    "serverless-prune-plugin": "^1.6.1",
    "serverless-webpack": "^5.6.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
